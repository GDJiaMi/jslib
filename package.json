{
  "name": "@gdjiami/jslib",
  "version": "0.4.2",
  "description": "Jiami FrontEnd helpers and Services",
  "main": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf lib",
    "build:es6": "tsc -d --outDir ./lib --module CommonJS ",
    "postbuild:es6": "cpx \"src/**/!(example)/*.+(css|jpg|jpeg|gif|png)\" ./lib",
    "build": "npm run build:es6 && cp package.json ./lib && cp README.md ./lib",
    "lint": "tslint -c tslint.json \"src/**/*.ts?(x)\"",
    "prettier": "prettier --config .prettierrc --write \"src/**/*.ts?(x)\"",
    "precommit": "lint-staged",
    "document": "typedoc --out docs/code/ src",
    "parcel": "parcel"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "npm run prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@code.ejiahe.com:25522/WEB/jm-jslib.git"
  },
  "keywords": [
    "jslib"
  ],
  "author": "ivan",
  "license": "MIT",
  "peerDependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/qs": "^6.5.1",
    "cpx": "^1.5.0",
    "husky": "^0.14.3",
    "jh-tslint": "^1.0.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "tslib": "^1.10.0",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-react": "3.2.0",
    "typedoc": "^0.15.3",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "qs": "^6.5.2"
  }
}
